{
  "name": "alhames/datetime",
  "type": "library",
  "description": "A simple PHP API extension for DateTime",
  "homepage": "https://github.com/alhames/datetime",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Logachev",
      "email": "alhames@mail.ru",
      "homepage": "https://alhames.ru/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "support": {
    "issues": "https://github.com/alhames/datetime/issues"
  },
  "autoload": {
    "psr-4": {
      "Alhames\\DateTime\\": ""
    },
    "files": ["functions.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "Alhames\\DateTime\\Tests\\": "Tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
